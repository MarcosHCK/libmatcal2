<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>MatcalSharp</id>
    <version>@PACKAGE_VERSION@</version>
    <authors>'MarcosHCK'</authors>
    <projectUrl>@PACKAGE_URL@</projectUrl>
    <description>MatcalSharp is a C# wrapper of libmatcal2</description>
    <license type="expression">GPL-3.0-or-later</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>libmatcal2 libmatcal wrapper</tags>
    <repository url="@PACKAGE_URL@" />
    <dependencies>
      <group targetFramework="net6.0">
        <dependency id="GLibSharp" version="3.24.24.38" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="../../../README.md" target="Readme.md" />
    <file src="lib/**" target="lib"/>
    <file src="runtimes/**" target="runtimes" />
  </files>
</package>
